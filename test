#!/bin/bash
source "lum.l"; STARTLUMFRAMEWORK

Class test {
        $..server() {
                echo "Content-Type: text/html"
                echo ""
                echo "200"
				
				declare -A obj;

				json.decode obj < json;

				obj[key1]="key 1 value";

				json.encode obj;

        }f


}c

HOST=localhost PORT=8080 http_server test;
